<assembly>
  <id>complete</id>
  <formats>
    <format>jar</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <dependencySets>
    <dependencySet>
      <outputDirectory>/</outputDirectory>
      <useProjectArtifact>true</useProjectArtifact>
      <unpack>true</unpack>
      <scope>runtime</scope>
    </dependencySet>
  </dependencySets>
  <fileSets>
    <fileSet>
      <directory>${project.build.directory}/jni</directory>
      <outputDirectory>/jni</outputDirectory>
      <includes>
        <include>**/*.dll</include>
        <include>**/*.so</include>
        <include>**/*.jnilib</include>
        <include>**/*.dylib</include>
        <include>**/*.a</include>
      </includes>
    </fileSet>
  </fileSets>
</assembly>